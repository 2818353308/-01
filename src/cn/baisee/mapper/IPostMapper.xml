<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper  
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.baisee.mapper.IPostMapper">
	<!-- 发帖方法 -->
	<insert id="post" parameterType="cn.baisee.entity.Gpaper">
		insert into	gpaper
		(g_type,g_content,g_image,g_time,gid)
		values(#{g_type},#{g_content},#{g_image},#{g_time},#{gid})
	</insert>
</mapper>